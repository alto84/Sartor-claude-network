{
  "version": "1.0.0",
  "mission": {
    "name": "Multi-Agent Framework Development",
    "description": "Build a robust memory, skills, research, and validation framework for agent bootstrapping",
    "start_time": "2025-12-15T17:20:00-05:00",
    "end_time": "2025-12-16T07:00:00-05:00",
    "final_report_time": "2025-12-16T06:30:00-05:00"
  },
  "memory_injection": {
    "enabled": true,
    "max_tokens": 2000,
    "topics": ["mission", "recent_findings", "framework_design"]
  },
  "skills_injection": {
    "always_load": ["validation", "bootstrap"],
    "role_based": {
      "researcher": ["research-web", "research-academic"],
      "orchestrator": ["coordination", "memory-query"],
      "implementer": ["validation"]
    }
  },
  "constraints": {
    "anti_fabrication": true,
    "check_time_before_spawn": true,
    "final_report_cutoff": "06:30",
    "max_child_agents": 5,
    "timeout_seconds": 300
  },
  "output_locations": {
    "results": ".swarm/results/",
    "artifacts": ".swarm/artifacts/",
    "research": ".swarm/artifacts/research/",
    "checkpoints": ".swarm/artifacts/checkpoints/"
  }
}
